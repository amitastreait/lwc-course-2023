<aura:component implements="flexipage:availableForAllPageTypes,lightning:isUrlAddressable" access="global">

    <aura:attribute type="String" name="codeValue"/>
    <aura:attribute type="String" name="codeText"/>
    <aura:attribute type="Object" name="userInfo"/>

    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    {!v.codeValue} {!v.codeText}
    <div class="slds-card">
        <c:targetComponent codeValue="{!v.codeValue}" codeText="{!v.codeText}" />
    </div>
</aura:component>	
